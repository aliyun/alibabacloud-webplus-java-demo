<!doctype html>
<html lang="en" data-framework="es6">
	<head>
		<meta charset="utf-8">
        <title>{{#i18n}}title{{/i18n}}</title>

		<!--<link rel="stylesheet" href="static/todomvc-app-css/index.css">-->
	<link href="../todo-list/bundle.css" rel="stylesheet"></head>
	<body>
		<section class="todoapp">
			<header class="header">
				<h1>{{#i18n}}todo.title{{/i18n}}</h1>
				<input class="new-todo" placeholder="{{#i18n}}what.needs.to.be.done{{/i18n}}" autofocus>
			</header>
			<section style="display:none" class="main">
				<input id="toggle-all" class="toggle-all" type="checkbox">
				<label for="toggle-all">Mark all as complete</label>
				<ul class="todo-list"></ul>
				<footer class="footer">
                    <span class="todo-count">{{#i18n}}items.left{{/i18n}}</span>
                    <span class="todo-count">&nbsp{{#i18n}}items.left{{/i18n}}</span>
					<ul class="filters">
						<li>
							<a href="#/" class="selected">{{#i18n}}all{{/i18n}}</a>
						</li>
						<li>
						<a href="#/active">{{#i18n}}active{{/i18n}}</a>
						</li>
						<li>
						<a href="#/completed">{{#i18n}}completed{{/i18n}}</a>
						</li>
					</ul>
					<button class="clear-completed">{{#i18n}}clear.completed{{/i18n}}</button>
				</footer>
			</section>
		</section>
		<footer class="info">
			<p>{{#i18n}}double.click.to.edit.a.todo{{/i18n}}</p>
			<!--<p>Written by <a href="http://twitter.com/lukeed05">Luke Edwards</a></p>-->
			<!--<p>Refactored by <a href="https://github.com/xorgy">Aaron Muir Hamilton</a></p>-->
			<p>Part of <a href="http://todomvc.com">TodoMVC</a></p>
		</footer>
		<!--<script src="index.js"></script>-->
		<!--<script src="static/todomvc-common/base.js"></script>-->
		<!--<link rel="stylesheet" href="static/todomvc-common/base.css">-->
	<script type="text/javascript" src="../todo-list/bundle.js"></script></body>
</html>
